{
    "name": "untitledui-nextjs-starter-kit",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "restart": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && bun dev",
        "kill:3000": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
        "build:worker": "node scripts/build-worker.js",
        "build:all": "npm run build && npm run build:worker",
        "deploy": "npm run build:worker && wrangler deploy",
        "deploy:preview": "npm run build:worker && wrangler deploy --env preview",
        "worker:dev": "wrangler dev",
        "worker:tail": "wrangler tail"
    },
    "dependencies": {
        "@tailwindcss/postcss": "^4.1.11",
        "@tailwindcss/typography": "^0.5.16",
        "@untitledui/file-icons": "^0.0.8",
        "@untitledui/icons": "^0.0.19",
        "motion": "^12.23.12",
        "next": "^15.4.6",
        "next-intl": "^4.3.6",
        "next-themes": "^0.4.6",
        "react": "19.1.1",
        "react-aria": "^3.42.0",
        "react-aria-components": "^1.12.0",
        "react-dom": "19.1.1",
        "react-hotkeys-hook": "^5.1.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tailwindcss-animate": "^1.0.7",
        "tailwindcss-react-aria-components": "^2.0.0"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/node": "^24.2.1",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "postcss": "^8.5.6",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "typescript": "^5.9.2",
        "wrangler": "^4.34.0"
    }
}
